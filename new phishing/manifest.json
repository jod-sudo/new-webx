{
    "manifest_version": 2,
    "name": "Phishing Blocker",
    "version": "1.0",
    "description": "Blocks phishing websites",
    "permissions": ["webRequest", "webNavigation", "activeTab","webRequestBlocking"],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "phish.html"
      
    },
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"] 
      }
    ]
  }
  